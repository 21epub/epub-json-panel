{
  "name": "@21epub/epub-json-panel",
  "version": "0.0.8",
  "description": "epub-json-panel",
  "author": "dxsixpc",
  "license": "MIT",
  "homepage": "https://github.com/21epub/epub-json-panel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/21epub/epub-json-panel.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "storybook",
    "typescript"
  ],
  "scripts": {
    "clean": "rm dist/bundle.js",
    "clean-build": "rm build/*",
    "start": "run-s storybook",
    "test": "jest --coverage",
    "lint": "run-s lint:js lint:style lint:prettier",
    "lint-staged:js": "eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:js": "eslint --fix --cache --ext .ts,.tsx ./src",
    "lint:prettier": "prettier --check --write \"**/*.{tsx,ts,less,md,json}\" --end-of-line auto",
    "lint:style": "stylelint --aei --fix \"src/**/*.less\" --syntax less",
    "start-w": "microbundle-crl watch --no-compress --format modern,cjs --sourcemap",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "prettier": "prettier -c --write \"**/*\"",
    "release": "git fetch --tags && npx standard-version",
    "release:alpha": "yarn run release -- --prerelease alpha",
    "commit": "lint-staged && npx git-cz",
    "prepublishOnly": "run-s lint test build",
    "install-peers": "npm-install-peers",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix --syntax less",
    "**/*.{ts,tsx}": "yarn run lint-staged:js",
    "**/*.{tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "antd": "^4.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@21epub/epub-form-components": "^0.3.8",
    "ahooks": "^2.10.0",
    "axios": "^0.21.1",
    "install-peers": "^1.0.3",
    "lodash": "^4.17.20",
    "monaco-editor-webpack-plugin": "^4.1.1",
    "react-use": "^17.2.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@storybook/addon-actions": "~6.3.8",
    "@storybook/addon-essentials": "~6.3.8",
    "@storybook/addon-links": "~6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.165",
    "@types/react": "16.9.50",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "autoprefixer": "9.8.6",
    "babel-jest": "26.3.0",
    "babel-loader": "8.2.2",
    "core-js": "^3.8.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.2.0",
    "husky": "4.3.0",
    "jest": "^27.0.3",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^10.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-install-peers": "^1.2.1",
    "npm-run-all": "4.1.5",
    "postcss-modules": "3.2.2",
    "prettier": "2.1.2",
    "queue-microtask": "^1.2.2",
    "sass": "^1.29.0",
    "sass-loader": "10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "ts-jest": "^27.0.2",
    "typescript": "4.0.3",
    "url-loader": "^4.1.1"
  },
  "files": [
    "dist"
  ]
}
